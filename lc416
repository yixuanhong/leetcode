class Solution:
    def canPartition(self, nums: List[int]) -> bool:
        total = sum(nums)
        if total % 2 != 0:
            return False
        
        target = total // 2
        dp = [False] * (target + 1)
        dp[0] = True
        
        for n in nums:
            for s in range(target, n - 1, -1):
                dp[s] |= dp[s - n]
        
        return dp[target]
